# Additional Test #
# Factorial (Cíclico + Pelos) #

function int factorial(int n) {
  set int i, result;
  result << 1;

  loop i in range (2 : 1 : i > n)
    result << result * i;

  return result;
}

function int pelos(int n) {
  return factorial(3 + n * 2);
}

main {
  set int fact, x;

  cwrite << "Número: ";
  cread << x;

  fact << pelos(x);
  cwrite << " ";
  cwrite << "Factorial: ";
  cwrite << fact;

  end
}