# MultiplicaciÃ³n de Matrices #

main {
  set int matrix a[2,4], b[4,3], m[2,3];
  set int i, j, k, x, y, z;

  a[0,0] << 1;
  a[0,1] << 2;
  a[0,2] << 3;
  a[0,3] << 4;
  a[1,0] << 5;
  a[1,1] << 6;
  a[1,2] << 7;
  a[1,3] << 8;
  b[0,0] << 1;
  b[0,1] << 2;
  b[0,2] << 3;
  b[1,0] << 4;
  b[1,1] << 5;
  b[1,2] << 6;
  b[2,0] << 7;
  b[2,1] << 8;
  b[2,2] << 9;
  b[3,0] << 10;
  b[3,1] << 11;
  b[3,2] << 12;
  m[0,0] << 0;
  m[0,1] << 0;
  m[0,2] << 0;
  m[1,0] << 0;
  m[1,1] << 0;
  m[1,2] << 0;

  loop i in range(0 : 1 : i = 2) {
    loop j in range(0 : 1 : j = 3) {
      loop k in range(0 : 1 : k = 4) {
        x << m[i,j];
        y << a[i,k];
        z << b[k,j];
        m[i,j] << x + y * z;
      }
    }
  }

  cwrite << "Matriz resultante:";
  cwrite << "{";
  loop i in range(0 : 1 : i = 2) {
    cwrite << "  {";
    loop j in range(0 : 1 : j = 3) {
      cwrite << m[i,j];
    }
    cwrite << "  }";
  }
  cwrite << "}";

  end
}