# ZenCode Test 3: #
# Logical Operations #

main {
  set int a, b, c;
  set bool comp;

  a << 6;
  b << 15;
  comp << {a > b};
  cwrite << comp;
  cwrite << {a < b};
  cwrite << {a = b};
  c << 6;
  cwrite << {a = c || a = b};
  cwrite << {a = c && a = b};
  cwrite << {a >< b};
  cwrite << {~ a >< b};
  cwrite << {a = c && ~ a = b};
  cwrite << {a <= c};
  cwrite << {b >= c};

  end
}